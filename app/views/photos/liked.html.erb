<div class="row">

  <div class="col-md-6 offset-md-3">
    
    <h1>
      <%= @user.username %>
    </h1>


    <ul class="nav nav-pills nav-justified">
      
      <li class="nav-item">
        <%= link_to "Posts", user_path(@user.username), class: "nav-link"%>
      </li>
      <li class="nav-item">
        <%= link_to "Likes", liked_photos_path(@user.username), class: "nav-link active"%>
      </li>
      <li class="nav-item">
        <%= link_to "Feed", user_feed_path(@user.username), class: "nav-link"%>
      </li>
      <li class="nav-item">
        <%= link_to "Followers", followers_path(@user.username), class: "nav-link"%>
      </li>
      <li class="nav-item">
        <%= link_to "Following", follows_path(@user.username), class: "nav-link"%>
      </li>
    </ul>
    <hr>
  </div>

</div>


<% @user.liked_photos.each do |photo| %>
  <%= render "photos/photo", photo: photo %>
<% end %>